    for (let key in changedExpenses) {\n      setTotalExpenses((oldTotalExpenses: number) => {\n        return (oldTotalExpenses += changedExpenses[key].amount);\n      });\n    }\n  };\n\n  useMemo(() => {\n    currencies && changeCurrency(currencies[0]);\n  }, []);\n\n  return (\n    <div className=\"card shadow-hover shadow\">\n      <div className=\"card-header py-3 d-flex flex-row align-items-center justify-content-between\">\n        <h6 className=\"m-0 font-weight-bold text-primary\">Expenses</h6>\n      </div>\n      <div className=\"btn-group currency-buttons\" role=\"group\">\n        {currencies.length >= 0 &&\n          currencies.map((currency, index) => {\n            return (\n              <button\n                key={index}\n                onClick={() => {\n                  changeCurrency(currency);\n                }}\n                type=\"button\"\n                className={'btn rounded-0 ' + 'btn-' + variant[index]}\n              >\n                {currency}\n              </button>\n            );\n          })}\n        {currencies.length > 1 && (\n          <button\n            type=\"button\"\n            onClick={() => {\n              setChangedExpenses(expensesCopy);\n              setTotalExpenses(0);\n            }}\n            className={'btn rounded-0 btn-link'}\n          >\n            Mixed\n          </button>\n        )}\n      </div>\n\n      <div className=\"card-body\">\n        {totalExpenses !== 0 && (\n          <div className=\"chart-pie pt-4 pb-2\">\n            <PieChart expenses={changedExpenses} />\n          </div>\n        )}\n        <div className=\"mt-4 text-center small\">\n          {changedExpenses.length > 0 &&\n            changedExpenses.map((expense: Expense, index: number) => {\n              return (\n                <div\n                  key={index}\n                  className=\"card-header py-3 d-flex flex-row align-items-center justify-content-around\"\n                >\n                  <span className=\"d-flex w-100 justify-content-between\">\n                    <FiberManualRecordIcon\n                      className=\"mr-3\"\n                      style={{ color: `${colors[index]}` }}\n                    />\n                    {expense.type + ' '}\n                    <div>\n                      {expense.amount + ' '}\n                      {expense.currency}\n                    </div>\n                  </span>\n                </div>\n              );\n            })}\n          {totalExpenses !== 0 && (\n            <div className=\"card-header py-3 d-flex flex-row align-items-center justify-content-around\">\n              <span className=\"d-flex w-100 justify-content-between\">\n                <FiberManualRecordIcon\n                  className=\"mr-3\"\n                  style={{ color: `${colors[48]}` }}\n                />\n                Total expenses\n                <div>\n                  {Math.round(totalExpenses * 100) / 100 + ' '}\n                  {changedExpenses[0].currency}\n                </div>\n              </span>\n            </div>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default SeeExpenses;\n","/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/Components/Topbar/FriendRequestDropdown.tsx",[],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/assets/ColorPalette.ts",[],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/Components/TripCards/Users/TripCardUser.tsx",[],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/Components/TripCards/MeTripCards/TripCardMe.tsx",["232"],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/Components/ModalCropPic/ImgCropper.jsx",["233","234","235","236","237"],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/Pages/Login/Login.tsx",[],"/Users/Caroline/Codeworks/SENIOR/legacy project/NextTripGo/client/src/services/Users/Users